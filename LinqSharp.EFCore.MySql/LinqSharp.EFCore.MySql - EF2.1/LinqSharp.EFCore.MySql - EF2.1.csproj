<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>preview</LangVersion>
    <PackageId>LinqSharp.EFCore.MySql</PackageId>
    <Authors>zmjack</Authors>
    <Product>LinqSharp.EFCore.MySql</Product>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <PackageIcon>LinqSharp.png</PackageIcon>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/zmjack/LinqSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/zmjack/LinqSharp</RepositoryUrl>
    <PackageTags>Linq EFCore MySql</PackageTags>
    <Copyright>Copyright Â© linqsharp.net 2020-2021</Copyright>
    <Version>2.1.6</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Expansion pack, including batch import function, etc.</Description>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;EFCORE2_1;EFCORE2_0_OR_GREATER;EFCORE2_1_OR_GREATER</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;EFCORE2_1;EFCORE2_0_OR_GREATER;EFCORE2_1_OR_GREATER</DefineConstants>
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MySqlConnector" Version="0.62.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\LinqSharp.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\LinqSharp.EFCore\LinqSharp.EFCore - EF2.1\LinqSharp.EFCore - EF2.1.csproj" />
  </ItemGroup>

  <Import Project="..\LinqSharp.EFCore.MySql\LinqSharp.EFCore.MySql.projitems" Label="Shared" />

</Project>
